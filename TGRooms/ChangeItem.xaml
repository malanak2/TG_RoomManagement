        xmlns:local="clr-namespace:TGRooms"
        mc:Ignorable="d"
        Title="ChangeItem" Height="450" Width="800">
    <Grid>
        <ComboBox HorizontalAlignment="Left" Margin="449,88,0,0" VerticalAlignment="Top" x:Name="room_combo" Width="120"/>
        <TextBox HorizontalAlignment="Left" Margin="306,91,0,0" TextWrapping="Wrap" Text="TextBox" x:Name="value_box" VerticalAlignment="Top" Width="120"/>
